{
    "name": "ageekdev/laravel-social-link-validator",
    "description": "",
    "keywords": [
        "ageekdev",
        "laravel-social-link-validator"
    ],
    "homepage": "https://github.com/ageekdev/laravel-social-link-validator",
    "license": "GPL-3.0",
    "type": "library",
    "authors": [
        {
            "name": "Ye Thu Soe",
            "email": "mr.yethusoe@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.23",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ageekdev\\LaravelSocialLinkValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ageekdev\\LaravelSocialLinkValidator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ageekdev\\LaravelSocialLinkValidator\\LaravelSocialLinkValidatorServiceProvider"
            ]
        }
    }
}
